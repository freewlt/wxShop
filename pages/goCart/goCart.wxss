/* pages/goCart/goCart.wxss */
.goCart{
    vertical-align:baseline;
    padding:0 0 2rem;
    border:0;
    font:inherit;
    font-size:100%;
    text-align:center;
    background:#f2f2f2;
}

.goCart .cart{        
    width: 4.83333rem;    
    height: 6.46rem;    
    background: transparent url(data:image/png;base64,) no-repeat;  
    background-size: 100%;    
    margin: 0 auto 1rem;
}  
.goCart .emptyText{
    font-size: 30rpx;
    color: #b3a078;
}
.shopBox{
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}
.shopBox .listBox{
    position: relative;
    width: 50%; 
    padding: 0 30rpx;
    box-sizing: border-box;
}
.listBox text{
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 26rpx;
}
.listBox .title{
    width:100%;
    padding: 16rpx 0 0;
}
.listBox .types{
    color: #999;
}
.listBox .feature{
    display: inline;
    padding: 5rpx 10rpx;
    background:#b3a078;
    color: #fff;
    font-size: 22rpx;
    border-radius: 10rpx;
}
.listBox .picBargain{
    width:200rpx;
    height:180rpx;
}
.cartShopping{
    width: 100%;
}